class Solution {
public:
    int numPairsDivisibleBy60(vector<int>& time) {
        vector<int> count(60, 0); 
        int result = 0;

        for (int t : time) {
            int r = t % 60;                  
            int needed = (60 - r) % 60;  
            result += count[needed];         
            count[r]++;                      
        }
        return result;
    }
};
